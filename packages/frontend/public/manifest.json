{
  "manifest_version": 3,
  "name": "vite-ts-react-test",
  "version": "1.0",
  "description": "",
  "action": {
    "default_title": "DIP"
  },
  "permissions": [
    "scripting",
    "tabs",
    "activeTab"
  ],
  "background": {
    "service_worker": "./js/background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "index.html"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}